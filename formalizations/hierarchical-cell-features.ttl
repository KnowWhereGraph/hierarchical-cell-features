@prefix : <http://ontologydesignpatterns.org/hierarchical-cell-features/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://ontologydesignpatterns.org/hierarchical-cell-features> .

<http://ontologydesignpatterns.org/hierarchical-cell-features> rdf:type owl:Ontology ;
                                                                owl:versionIRI <http://ontologydesignpatterns.org/hierarchical-cell-features/0.0.1> .

#################################################################
#    Annotation properties
#################################################################

###  http://ontologydesignpatterns.org/opla-sd#entityPositionX
<http://ontologydesignpatterns.org/opla-sd#entityPositionX> rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionY
<http://ontologydesignpatterns.org/opla-sd#entityPositionY> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ontologydesignpatterns.org/hierarchical-cell-features/cellRolified
:cellRolified rdf:type owl:ObjectProperty .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/contains
:contains rdf:type owl:ObjectProperty .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/hasAttribute
:hasAttribute rdf:type owl:ObjectProperty .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/hasGeometry
:hasGeometry rdf:type owl:ObjectProperty .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/hasNonEmptyOverlap
:hasNonEmptyOverlap rdf:type owl:ObjectProperty .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/hasValue
:hasValue rdf:type owl:ObjectProperty .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/isFullyContainedIn
:isFullyContainedIn rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasNonEmptyOverlap .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/isSubcellOf
:isSubcellOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isFullyContainedIn .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/overlaps
:overlaps rdf:type owl:ObjectProperty ;
          owl:propertyChainAxiom ( :contains
                                   :cellRolified
                                   [ owl:inverseOf :contains
                                   ]
                                 ) ,
                                 ( :contains
                                   :isSubcellOf
                                 ) ,
                                 ( :contains
                                   :regionRolified
                                   [ owl:inverseOf :contains
                                   ]
                                 ) .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/regionRolified
:regionRolified rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://ontologydesignpatterns.org/hierarchical-cell-features/Attribute
:Attribute rdf:type owl:Class ;
           rdfs:subClassOf :EntityWithProvenance ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasValue ;
                             owl:allValuesFrom :Quantity
                           ] .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/Cell
:Cell rdf:type owl:Class ;
      rdfs:subClassOf :Geometry ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :cellRolified ;
                        owl:hasSelf "true"^^xsd:boolean
                      ] .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/EntityWithProvenance
:EntityWithProvenance rdf:type owl:Class .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/Feature
:Feature rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasAttribute ;
                           owl:allValuesFrom :Attribute
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasGeometry ;
                           owl:allValuesFrom :Geometry
                         ] .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/Geometry
:Geometry rdf:type owl:Class .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/Quantity
:Quantity rdf:type owl:Class .


###  http://ontologydesignpatterns.org/hierarchical-cell-features/Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasGeometry ;
                          owl:allValuesFrom :Geometry
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :regionRolified ;
                          owl:hasSelf "true"^^xsd:boolean
                        ] .

#################################################################
#    General axioms
#################################################################

[ rdf:type owl:Restriction ;
   owl:onProperty :hasAttribute ;
   owl:someValuesFrom :Attribute ;
   rdfs:subClassOf :Feature
 ] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasAttribute ;
  owl:someValuesFrom :Attribute ;
  rdfs:subClassOf :Region
] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasGeometry ;
  owl:someValuesFrom :Geometry ;
  rdfs:subClassOf :Feature
] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasGeometry ;
  owl:someValuesFrom :Geometry ;
  rdfs:subClassOf :Region
] .


[ rdf:type owl:Restriction ;
  owl:onProperty :hasValue ;
  owl:someValuesFrom :Quantity ;
  rdfs:subClassOf :Attribute
] .


